<?xml version="1.0" encoding="UTF-8"?>
<rest xmlns="http://xtrip.gridnine.com/jasmine/rest" id="demo">
    <group id="demo">
        <operation id="changePassword" handler="com.gridnine.jasmine.server.demo.rest.DemoChangePasswordRestHandler">
            <request id="com.gridnine.jasmine.server.demo.rest.DemoChangePasswordRequest">
                <property id="userAccountUid" type="STRING" lateinit="true"/>
                <property id="vm" type="ENTITY" class-name="com.gridnine.jasmine.web.demo.DemoChangePasswordEditorVM" lateinit="true"/>
            </request>
            <response id="com.gridnine.jasmine.server.demo.rest.DemoChangePasswordResponse">
                <property id="success" type="BOOLEAN" non-nullable="true"/>
                <property id="vv" type="ENTITY" class-name="com.gridnine.jasmine.web.demo.DemoChangePasswordEditorVV"/>
                <property id="message" type="ENTITY" class-name="com.gridnine.jasmine.server.standard.rest.Message" lateinit="true"/>
            </response>
        </operation>
        <operation id="createUser" handler="com.gridnine.jasmine.server.demo.rest.DemoCreateUserRestHandler">
            <request id="com.gridnine.jasmine.server.demo.rest.DemoCreateUserRequest">
                <property id="vm" type="ENTITY" class-name="com.gridnine.jasmine.web.demo.DemoUserNewAccountEditorVM" lateinit="true"/>
            </request>
            <response id="com.gridnine.jasmine.server.demo.rest.DemoCreateUserResponse">
                <property id="vv" type="ENTITY" class-name="com.gridnine.jasmine.web.demo.DemoUserNewAccountEditorVV"/>
                <property id="objectUid" type="STRING"/>
            </response>
        </operation>
    </group>

</rest>


