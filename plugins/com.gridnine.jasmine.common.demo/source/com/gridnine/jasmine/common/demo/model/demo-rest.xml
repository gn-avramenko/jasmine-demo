<?xml version="1.0" encoding="UTF-8"?>
<rest xmlns="http://gridnine.com/jasmine/rest" id="demo">
    <group id="demo">
        <operation id="createUser" handler="com.gridnine.jasmine.server.demo.rest.DemoCreateUserRestHandler">
            <request id="com.gridnine.jasmine.common.demo.rest.DemoCreateUserRequest">
                <property id="vm" type="ENTITY" class-name="com.gridnine.jasmine.common.demo.model.ui.DemoUserNewAccountEditorVM" lateinit="true"/>
            </request>
            <response id="com.gridnine.jasmine.common.demo.rest.DemoCreateUserResponse">
                <property id="vv" type="ENTITY" class-name="com.gridnine.jasmine.common.demo.model.ui.DemoUserNewAccountEditorVV"/>
                <property id="objectUid" type="STRING"/>
            </response>
        </operation>
        <operation id="throwException" handler="com.gridnine.jasmine.server.demo.rest.DemoThrowExceptionRestHandler">
            <request id="com.gridnine.jasmine.common.demo.rest.DemoThrowExceptionRequest">
            </request>
            <response id="com.gridnine.jasmine.common.demo.rest.DemoThrowExceptionResponse">
            </response>
        </operation>
    </group>

</rest>


